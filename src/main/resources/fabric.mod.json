{
  "schemaVersion": 1,
  "id": "volumebetaextension",
  "version": "${version}",

  "name": "Volume Beta Extension",
  "description": "This mod is supposed to add music from Minecraft: Volume Beta to the game",
  "authors": [
    "tepiloxtl"
  ],
  "contact": {
    "homepage": "https://tepiloxtl.net/",
    "sources": "https://github.com/tepiloxtl/"
  },

  "license": "CC0-1.0",
  "icon": "assets/volumebetaextension/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "net.tepiloxtl.volumebetaextension.VolumeBetaExtension"
    ],
    "stationapi:event_bus_client": [
    ],
    "stationapi:event_bus_server": [
    ]
  },
  "mixins": [
    "volumebetaextension.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
